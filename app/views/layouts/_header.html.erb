<header class="header">

      <%= image_tag("Site/game-land-logo.png") %>
      
      <ul>
        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "All Games", items_path %></li>
        <% Console.all.each do |console| %>
        <li><a href="/console/<%= console.title %>"><%= console.title %></a></li>
        <% end %>
        <li><%= link_to "Cart", cart_path  %></li>

        <span class="right-nav">
        <li><%= link_to "About", about_path %></li>
        <li><%= link_to "Help", help_path %></li>
        <% if user_signed_in? %>
        <li><%= link_to 'Edit Profile', edit_user_registration_path %></li>
        <li><%= link_to 'Sign Out', destroy_user_session_path, :method =>:delete %></li>
        
          <% if current_user.admin? %> 
          <li class = "isAdminLi">Admin</li>
          <% else  %>
          <li class = "isAdminLi">User</li>
          <% end %>
        <% else %>
        <li><%= link_to 'Register', new_user_registration_path %></li>
        <li><%= link_to 'Sign In', new_user_session_path %></li>
   
        <% end %>
        </span>
      </ul>
      
</header>